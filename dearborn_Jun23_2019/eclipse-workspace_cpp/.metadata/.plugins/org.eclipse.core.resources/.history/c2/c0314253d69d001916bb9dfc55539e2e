#include <iostream>

using namespace std;

class GradeBook {
public:
	void displayMessage(){
		cout<<"This method of displayMessage is to display the instance of this object."<<endl;
	}
};

int main(){
	GradeBook myGradeBook;
	myGradeBook.displayMessage();
}


