#include <iostream>

using namespace std;

int main(){
	int number1, number2;

	cout<<"Pls enter two integers to compare: ";
	cin>>number1>>number2;

	if (number1==number2) cout<<number1 <<" == "<<number2<<endl;
	if (number1!=number2) cout<<number1 <<" != "<<number2<<endl;
	if (number1>number2) cout<<number1 <<" > "<<number2<<endl;
	if (number1<number2) cout<<number1 <<" < "<<number2<<endl;

}

